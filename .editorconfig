[*.cs]

# IDE1006: Estilos de nombres
dotnet_diagnostic.IDE1006.severity = none

# IDE0060: Quitar el parámetro no utilizado
dotnet_code_quality_unused_parameters = all:none

# IDE0044: Agregar modificador de solo lectura
dotnet_style_readonly_field = true:none

# IDE0051: Quitar miembros privados no utilizados
dotnet_diagnostic.IDE0051.severity = none

# IDE0052: Quitar miembros privados no leídos
dotnet_diagnostic.IDE0052.severity = none

# IDE0059: Asignación innecesaria de un valor
csharp_style_unused_value_assignment_preference = discard_variable:none

# IDE0055: Fijar formato
dotnet_diagnostic.IDE0055.severity = none
